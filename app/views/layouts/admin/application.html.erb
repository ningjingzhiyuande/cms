  <script type="text/javascript"  >
function showsubmenu(sid)
{
  whichEl = $("#submenu" + sid);
  if (whichEl.css("display") == "none")
  {
     $("#submenu" + sid).attr("style","display:block;");
  }
else
  {
     $("#submenu" + sid).attr("style","display:none;");
  }
}
</script>
